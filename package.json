{
  "name": "@bernankez/prompt",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@8.6.5",
  "description": "Effortlessly build beautiful command-line apps",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/bernankez/prompt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bernankez/prompt.git"
  },
  "bugs": "https://github.com/bernankez/prompt/issues",
  "keywords": [
    "clack",
    "command-line",
    "cli",
    "prompt"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "watch": "tsx --watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "dependencies": {
    "@clack/core": "^0.3.2",
    "picocolors": "^1.0.0",
    "sisteransi": "^1.0.5"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.0",
    "@types/node": "^20.3.3",
    "bumpp": "^9.1.1",
    "eslint": "^8.44.0",
    "is-unicode-supported": "^1.3.0",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundledDependencies": [
    "is-unicode-supported"
  ]
}
